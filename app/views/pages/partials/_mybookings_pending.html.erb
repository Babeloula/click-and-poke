<div class="tab-pane fade show active" id="home" role="tabpanel" aria-labelledby="home-tab">
  <div id="card-product-pending-mybook-flex">
    <% @bookings.status_Pending.each do |booking| %>
      <div class="card-product">
        <%= link_to image_tag("https://upload.wikimedia.org/wikipedia/commons/5/51/Pokebola-pokeball-png-0.png"), booking_path(booking) %>
        <div class="card-product-infos">
          <p><strong>Rented Pokespot : </strong><%= booking.pokespot.name %></p>
          <p><strong>Client name : </strong><%= booking.user.username%></p>
          <p><strong>Start date : </strong><%= booking.start_date %></p>
          <p><strong>End date : </strong><%= booking.end_date %></p>
          <a id="pending-background">Pending</a>
        </div>
      </div>
    <% end %>
  </div>
</div>
